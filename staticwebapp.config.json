{
	"navigationFallback": {
		"rewrite": "/index.html",
		"exclude": ["*.{css,sccs,js,png,gif,ico,jpg,svg}"]
	},
	"routes": [
	  {
		"route":"/data",
		"redirect":"/contacto",
		"statusCode": 301
	  },
	  {
		"route": "/login",
		"redirect": "/.auth/login/github"
	  },
	  {
		"route": "/logout",
		"redirect": "/.auth/logut"
	  },
	  {
		"route": "/api/*",
		"allowedRoles": ["authenticated"]
	  },
	  {
		"route": "/contacto",
		"allowedRoles": ["authenticated"]
	  }
	]
}
